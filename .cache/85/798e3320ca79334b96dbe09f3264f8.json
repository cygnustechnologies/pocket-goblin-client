":4},"source":"components/TopSpendingGraphs.jsx","original":{"line":67,"column":4}},{"generated":{"line":96,"column":6},"source":"components/TopSpendingGraphs.jsx","original":{"line":107,"column":6}},{"generated":{"line":96,"column":322},"source":"components/TopSpendingGraphs.jsx","original":{"line":67,"column":4}},{"generated":{"line":96,"column":323},"source":"components/TopSpendingGraphs.jsx","original":{"line":66,"column":2}},{"generated":{"line":97,"column":0},"source":"components/TopSpendingGraphs.jsx","original":{"line":115,"column":1}},{"generated":{"line":97,"column":1},"source":"components/TopSpendingGraphs.jsx","original":{"line":10,"column":0}},{"name":"TopSpendingGraphs","generated":{"line":99,"column":15},"source":"components/TopSpendingGraphs.jsx","original":{"line":117,"column":15}}],"sources":{"components/TopSpendingGraphs.jsx":"import React from \"react\";\r\nimport { HorizontalBar } from \"react-chartjs-2\";\r\nimport styled from \"react-emotion\";\r\n\r\n//Styled Components\r\nconst Titles = styled(\"h1\")`\r\n  text-align: center;\r\n`;\r\n\r\nconst TopSpendingGraphs = ({ recurring, categories, shop }) => {\r\n  const barGraphColors = [\r\n    \"rgb(146, 66, 244)\",\r\n    \"rgb(65, 86, 244)\",\r\n    \"rgb(235, 244, 65)\",\r\n    \"rgb(244, 65, 184)\"\r\n  ];\r\n\r\n  const options = {\r\n    legend: {\r\n      display: false\r\n    },\r\n    scales: {\r\n      xAxes: [\r\n        {\r\n          ticks: {\r\n            beginAtZero: true\r\n          }\r\n        }\r\n      ]\r\n    }\r\n  };\r\n\r\n  const recurringGraph = {\r\n    labels: recurring.labels,\r\n    datasets: [\r\n      {\r\n        label: \"Top Recurring\",\r\n        backgroundColor: barGraphColors,\r\n        data: recurring.data\r\n      }\r\n    ]\r\n  };\r\n\r\n  const categoriesGraph = {\r\n    labels: categories.labels,\r\n    datasets: [\r\n      {\r\n        label: \"Top Categories\",\r\n        backgroundColor: barGraphColors,\r\n        data: categories.data\r\n      }\r\n    ]\r\n  };\r\n\r\n  const shopGraph = {\r\n    labels: shop.labels,\r\n    datasets: [\r\n      {\r\n        label: \"Top Shop\",\r\n        backgroundColor: barGraphColors,\r\n        data: shop.data\r\n      }\r\n    ]\r\n  };\r\n\r\n  return (\r\n    <div\r\n      style={{ height: \"50%\", width: \"50%\" }}\r\n      data-test=\"top-spending-graphs\"\r\n    >\r\n      <Titles>Top Recurring </Titles>\r\n      <select>\r\n        <option value=\"monthly\">Monthly</option>\r\n      </select>\r\n      <HorizontalBar\r\n        data={recurringGraph}\r\n        options={options}\r\n        data-type=\"horizontal-bars\"\r\n      />\r\n      <p>\r\n        Recurring expenses add up! Click on one to analyze how reducting it\r\n        would help imporve your financial health.\r\n      </p>\r\n      <Titles>Top Categories </Titles>\r\n      <select>\r\n        <option value=\"monthly\">Monthly</option>\r\n      </select>\r\n      <HorizontalBar\r\n        data={categoriesGraph}\r\n        options={options}\r\n        data-type=\"horizontal-bars\"\r\n      />\r\n      <p>\r\n        Category spending shows you genral trends. Most people can save tons of\r\n        money be reducing the amount of money they spend at bars and\r\n        restaurants.\r\n      </p>\r\n      <Titles>Top Shop </Titles>\r\n      <select>\r\n        <option value=\"monthly\">Monthly</option>\r\n      </select>\r\n      <HorizontalBar\r\n        data={shopGraph}\r\n        options={options}\r\n        data-type=\"horizontal-bars\"\r\n      />\r\n      <p>\r\n        {`Watch out! That daily latte or impulsive Amazon purchase adds up! If you\r\n        used that money to pay down debt or top up savings, you will be in far\r\n        better shape! And let's be honest, I'll bet you wouldnt miss that shiny\r\n        new gadget after a few months.`}\r\n      </p>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default TopSpendingGraphs;\r\n"},"lineCount":null}},"hash":"f2adcdf30cf44588de32b1758913ef5b","cacheData":{"env":{}}}