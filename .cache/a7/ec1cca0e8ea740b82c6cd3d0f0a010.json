erated":{"line":116,"column":75},"source":"components/EnterPurchase.jsx","original":{"line":32,"column":6}},{"generated":{"line":116,"column":76},"source":"components/EnterPurchase.jsx","original":{"line":31,"column":4}},{"generated":{"line":117,"column":0},"source":"components/EnterPurchase.jsx","original":{"line":82,"column":3}},{"name":"React","generated":{"line":121,"column":2},"source":"components/EnterPurchase.jsx","original":{"line":15,"column":28}},{"name":"Component","generated":{"line":121,"column":17},"source":"components/EnterPurchase.jsx","original":{"line":15,"column":34}},{"name":"EnterPurchase","generated":{"line":123,"column":15},"source":"components/EnterPurchase.jsx","original":{"line":85,"column":15}}],"sources":{"components/EnterPurchase.jsx":"import React from \"react\";\r\nimport { Link } from \"react-router-dom\";\r\nimport styled from \"react-emotion\";\r\nimport InputAmount from \"./InputAmount.jsx\";\r\nimport CashFlowMeter from \"./CashFlowMeter.jsx\";\r\n\r\n//Styled Components\r\nconst Wrapper = styled(\"div\")`\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n  text-align: center;\r\n`;\r\n\r\nclass EnterPurchase extends React.Component {\r\n  constructor(props) {\r\n    super(props);\r\n  }\r\n\r\n  render() {\r\n    const {\r\n      handlePurchaseInput,\r\n      currentCashFlowAmount,\r\n      purchaseFrequency,\r\n      handlePurchaseFrequencyChange,\r\n      purchasePaymentType,\r\n      purchaseAmount,\r\n      handlePaymentTypeChange\r\n    } = this.props;\r\n\r\n    return (\r\n      <Wrapper data-test=\"enter-purchase\">\r\n        <h1>Enter potential purchase</h1>\r\n        <InputAmount\r\n          data-test=\"input-amount\"\r\n          handlePurchaseInput={handlePurchaseInput}\r\n          purchaseAmount={purchaseAmount}\r\n        />\r\n        <div>\r\n          <h2>Cash Flow</h2>\r\n          <CashFlowMeter\r\n            data-test=\"cash-flow-meter\"\r\n            currentCashFlowAmount={currentCashFlowAmount}\r\n            purchaseAmount={purchaseAmount}\r\n          />\r\n        </div>\r\n        <div style={{ margin: \"2%\" }}>\r\n          Repeat:\r\n          <select\r\n            value={purchaseFrequency}\r\n            onBlur={handlePurchaseFrequencyChange}\r\n            onChange={handlePurchaseFrequencyChange}\r\n          >\r\n            <option value=\"never\">Never</option>\r\n            <option value=\"monthly\">Monthly</option>\r\n          </select>\r\n        </div>\r\n\r\n        <div style={{ margin: \"2%\", marginBottom: \"2%\" }}>\r\n          <input\r\n            name=\"paymentType\"\r\n            type=\"radio\"\r\n            value=\"cash\"\r\n            checked={purchasePaymentType === \"cash\"}\r\n            onChange={handlePaymentTypeChange}\r\n          />\r\n          Pay in Cash\r\n          <input\r\n            name=\"paymentType\"\r\n            type=\"radio\"\r\n            value=\"credit\"\r\n            checked={purchasePaymentType === \"credit\"}\r\n            onChange={handlePaymentTypeChange}\r\n          />\r\n          Pay with Credit\r\n        </div>\r\n        <Link to=\"/snapshot-results\">\r\n          <button>Start the Goblin!</button>\r\n        </Link>\r\n      </Wrapper>\r\n    );\r\n  }\r\n}\r\n\r\nexport default EnterPurchase;\r\n"},"lineCount":null}},"hash":"b9bef4cc5f00ed7bac9532d04a9e518e","cacheData":{"env":{}}}